{
  "extends": [
    "react-app",
    "eslint:recommended",
    "airbnb",
    "semistandard-react",
    "prettier"
  ],
  "rules": {
    "import/prefer-default-export": 0,
    "semistandard-react/prop-types": 0,
    "react/prop-types": 0,
    "react/no-array-index-key": 0,
    "react/jsx-props-no-spreading": 0,
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "settings": {
    "react": {
      "version": "16.13.1"
    }
  }
}
